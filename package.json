{
  "name": "page-insights",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist",
    "javascript",
    "src/**/*.cnd",
    "settings",
    "static"
  ],
  "scripts": {
    "build": "tsc --noEmit && vite build && run package",
    "clean": "rm -rf dist/",
    "deploy": "jahia-deploy",
    "dev": "vite build --watch",
    "format": "prettier --write --list-different .",
    "lint": "eslint .",
    "package": "yarn pack --out dist/package.tgz",
    "watch": "run dev",
    "watch:callback": "run package && run deploy"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^2.0.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.26.0",
    "@jahia/vite-plugin": "^1.1.0",
    "@types/prop-types": "^15",
    "@types/react": "^19.1.2",
    "@types/react-redux": "^7.1.34",
    "eslint": "^9.26.0",
    "globals": "^16.0.0",
    "i18next": "^25.0.2",
    "prettier": "^3.5.3",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-i18next": "^16.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^7.0.0",
    "vitest": "^4.0.16"
  },
  "packageManager": "yarn@4.10.3",
  "engines": {
    "node": ">=22.0.0"
  },
  "jahia": {
    "name": "page-insights",
    "snapshot": true,
    "module-dependencies": "default,jexperience",
    "module-type": "module",
    "static-resources": "/javascript/apps,/dist/client,/dist/assets,/locales,/images,/icons"
  },
  "dependencies": {
    "@jahia/data-helper": "^1.1.16",
    "@jahia/ui-extender": "^1.2.0",
    "@jahia/vite-federation-plugin": "^0.1.0",
    "chart.js": "^4.4.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0"
  }
}
