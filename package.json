{
  "name": "@smonier/page-insights",
  "version": "1.0.0",
  "description": "Displays real-time analytics and visitor insights for Jahia pages directly in the jContent interface. Provides key metrics like page views, unique visitors, sessions, and time-series trends with seamless integration to Kibana dashboards for detailed analysis.",
  "main": "dist/server/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/smonier/page-insights.git"
  },
  "keywords": [
    "jahia",
    "module",
    "page-insights"
  ],
  "author": "St√©phane Monier <smonier@jahia.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smonier/page-insights/issues"
  },
  "homepage": "https://github.com/smonier/page-insights",
  "type": "module",
  "files": [
    "dist",
    "javascript",
    "src/**/*.cnd",
    "settings",
    "static"
  ],
  
  "scripts": {
    "build": "tsc --noEmit && vite build && run package",
    "clean": "rm -rf dist/",
    "deploy": "jahia-deploy",
    "dev": "vite build --watch",
    "format": "prettier --write --list-different .",
    "lint": "eslint .",
    "package": "yarn pack --out dist/package.tgz",
    "watch": "run dev",
    "watch:callback": "run package && run deploy"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^2.0.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.26.0",
    "@jahia/vite-plugin": "^1.1.0",
    "@types/prop-types": "^15",
    "@types/react": "^19.1.2",
    "eslint": "^9.26.0",
    "globals": "^16.0.0",
    "i18next": "^25.0.2",
    "prettier": "^3.5.3",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-i18next": "^16.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^7.0.0",
    "vitest": "^4.0.16"
  },
  "packageManager": "yarn@4.10.3",
  "engines": {
    "node": ">=22.0.0"
  },
  "jahia": {
    "required-version": "8.2.1.0",
    "name": "page-insights",
    "snapshot": false,
    "module-dependencies": "default,jexperience,javascript-modules-engine=[1,2)",
    "module-type": "module",
    "static-resources": "/javascript/apps,/dist/client,/dist/assets,/locales,/images,/icons",
    "server": "dist/server/index.js",
    "maven": {
      "groupId": "org.jahia.modules.se.javascript",
      "distributionManagement": {
        "repository": {
          "id": "jahia-releases",
          "url": "https://devtools.jahia.com/nexus/content/repositories/jahia-releases"
        },
        "snapshotRepository": {
          "id": "jahia-snapshots",
          "url": "https://devtools.jahia.com/nexus/content/repositories/jahia-snapshots"
        }
      }
    }
  },
  "dependencies": {
    "@jahia/data-helper": "^1.1.14",
    "@jahia/moonstone": "^2.17.3",
    "@jahia/ui-extender": "^1.2.0",
    "@jahia/vite-federation-plugin": "^0.1.0",
    "chart.js": "^4.4.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-table": "^7.8.0"
  }
}
